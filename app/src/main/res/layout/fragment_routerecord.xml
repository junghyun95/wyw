<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".RouteRecordFragment">

    <TextView
            android:text="현재 위치"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textSize="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="32dp"
            app:layout_constraintVertical_bias="0.0"/>

    <com.google.android.gms.maps.MapView
            android:id="@+id/mapview"
            android:layout_width="344dp"
            android:layout_height="325dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.047"/>
    <Button
            android:text="renew"
            android:layout_width="67dp"
            android:layout_height="35dp"
            android:id="@+id/btnRenew"
            android:backgroundTint="@color/wywColor"
            android:textSize="8sp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.902" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="56dp" app:layout_constraintBottom_toTopOf="@+id/mapview"
            app:layout_constraintVertical_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>