<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content">

    <ImageView
            android:layout_width="51dp"
            android:layout_height="59dp" app:srcCompat="@drawable/alerticon" android:id="@+id/imageView3"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="16dp"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="날짜"
            android:layout_width="152dp"
            android:layout_height="19dp"
            android:id="@+id/date"
            android:textColor="@color/wywColor"
            android:fontFamily="@font/chosungu"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="4dp"
            android:textSize="15sp" android:layout_marginEnd="172dp"
            app:layout_constraintStart_toEndOf="@+id/imageView3" app:layout_constraintBottom_toTopOf="@+id/lapTime"/>
    <TextView
            android:text="랩타임 기록"
            android:layout_width="259dp"
            android:layout_height="28dp"
            android:id="@+id/lapTime"
            android:textColor="#000000"
            android:textSize="20sp"
            android:fontFamily="@font/chosungu"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="64dp"
            app:layout_constraintStart_toEndOf="@+id/imageView3" app:layout_constraintHorizontal_bias="0.428"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="x"
            android:textColor="@color/wywColor"
            android:layout_width="44dp"
            android:layout_height="42dp"
            android:id="@+id/btnDelete"
            android:backgroundTint="@android:color/white"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/lapTime"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:textSize="20sp"/>
    <TextView
            android:text="no"
            android:layout_width="18dp"
            android:layout_height="21dp"
            android:id="@+id/num"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/date" android:layout_marginTop="20dp"
            app:layout_constraintHorizontal_bias="0.559" android:textColor="#000000"/>
</androidx.constraintlayout.widget.ConstraintLayout>